
<%= form_for @recipe do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %></br>

    <%= f.fields_for :ingredients do |ingreds| %>
        <%= ingreds.label :name %>
        <%= ingreds.text_field :name %>
        <%= ingreds.label :quantity %>
        <%= ingreds.text_field :quantity %>

        <%= f.submit %>
        <%end%>